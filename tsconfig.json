{
  "compilerOptions": {
    "target": "es2016",
    "baseUrl": ".",
    "module": "commonjs",
    "outDir": "./build",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "jsx": "react-native",
    "paths": {
      "assets/*": [
        "assets/*"
      ],
      "components/*": [
        "components/*"
      ],
      "screens/*": [
        "screens/*"
      ]
    }
  },
  "include": [
    "./**/*"
  ],
  "exclude": [
    "api/*"
  ],
  "extends": "expo/tsconfig.base",
  "typeRoots": [
    "env.d.ts"
  ]
}
