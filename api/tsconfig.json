{
  "compilerOptions": {
    "target": "es2016",
    "baseUrl": "src",
    "module": "commonjs",
    "outDir": "./build",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "paths": {
    }
  },
  "include": [
    "src/**/*"
  ]
}
